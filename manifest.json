{
    "manifest_version": 3,
    "name": "AdBlocker",
    "version": "1.0",
    "description": "Blocks ads on websites.",
    "permissions": ["scripting", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "idea.png",
      "48": "idea.png",
      "128": "idea.png"
    },
    "action": {
      "default_icon": "idea.png",
      "default_title": "AdBlocker"
    }
  }
  